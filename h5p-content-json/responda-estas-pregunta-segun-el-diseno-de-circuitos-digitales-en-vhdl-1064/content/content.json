                            "params": {
                                "question": "\u00bfQu\u00e9 opci\u00f3n de sintaxis es correcta para definir una se\u00f1al de 8 bits en VHDL?",
                                "answers": [
                                    {
                                        "text": "<div>reg [7:0] my_signal;<\/div>\n",
                                        "correct": true,
                                        "tipsAndFeedback": {
                                            "tip": "",
                                            "chosenFeedback": "En VHDL, \"reg\" se utiliza para declarar se\u00f1ales internas.",
                                            "notChosenFeedback": "Correcto."
                                        }
                                    },
                                    {
                                        "text": "<div>signal reg [7:0] my_signal;<\/div>\n",
                                        "correct": false,
                                        "tipsAndFeedback": {
                                            "tip": "",
                                            "chosenFeedback": "La palabra \"signal\" no se utiliza antes de la declaraci\u00f3n de tipo.",
                                            "notChosenFeedback": "Incorrecto."
                                        }
                                    },
                                    {
                                        "text": "wire [8] my_signal;",
                                        "correct": false,
                                        "tipsAndFeedback": {
                                            "tip": "En VHDL, se utiliza \"wire\" en Verilog para declarar conexiones de cables.",
                                            "chosenFeedback": "Incorrecto.",
                                            "notChosenFeedback": ""
                                        }
                                    }
