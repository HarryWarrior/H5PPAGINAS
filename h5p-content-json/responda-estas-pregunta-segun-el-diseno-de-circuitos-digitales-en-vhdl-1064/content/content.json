                            "params": {
                                "question": "\u00bfCu\u00e1l es la funci\u00f3n del acarreo de salida en el circuito combinacional de suma binaria?",
                                "answers": [
                                    {
                                        "text": "Llevar el bit que se lleva cuando una suma excede el rango de bits disponibles.",
                                        "correct": true,
                                        "tipsAndFeedback": {
                                            "tip": "El acarreo de salida es importante para manejar sumas que exceden el rango de bits disponibles en el resultado.",
                                            "chosenFeedback": "Correcto.",
                                            "notChosenFeedback": ""
                                        }
                                    },
                                    {
                                        "text": "Indicar si la suma es positiva o negativa.",
                                        "correct": false,
                                        "tipsAndFeedback": {
                                            "tip": "El acarreo de salida no indica la polaridad de la suma.",
                                            "chosenFeedback": "Incorrecto.",
                                            "notChosenFeedback": ""
                                        }
                                    },
                                    {
                                        "text": "Determinar el tipo de operaci\u00f3n de suma (por ejemplo, suma o resta).",
                                        "correct": false,
                                        "tipsAndFeedback": {
                                            "tip": "El acarreo de salida no determina si la operaci\u00f3n es una suma o una resta.",
                                            "chosenFeedback": "Incorrecto.",
                                            "notChosenFeedback": ""
                                        }
